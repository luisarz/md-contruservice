{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "barryvdh/laravel-dompdf": "^v3.1.1",
    "bezhansalleh/filament-shield": "^4.0.2",
    "codewithdennis/filament-simple-alert": "4.x",
    "filament/filament": "^v4.1.8",
    "hydrat/filament-table-layout-toggle": "3.0",
    "laravel/framework": "v12.29.0",
    "laravel/tinker": "^2.9",
    "livewire/livewire": "^3.4",
    "livewire/volt": "^1.0",
    "luecano/numero-a-letras": "^v4.0.1",
    "malzariey/filament-daterangepicker-filter": "^4.0.6",
    "marcelweidum/filament-expiration-notice": "^v1.0.9",
    "mpdf/mpdf": "^8.2",
    "psr/simple-cache": "^3.0",
    "pxlrbt/filament-excel": "3.0",
    "saade/filament-laravel-log": "^4.0",
    "schmeits/filament-4-phosphor-icons": "^1.0.6",
    "simplesoftwareio/simple-qrcode": "^4.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "filament/upgrade": "4.0",
    "laravel/breeze": "^2.2",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.1",
    "phpunit/phpunit": "^11.0.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/DteHelper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
